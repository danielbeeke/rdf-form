{
  "scripts": {
    "test": "cypress open",
    "start": "serve --ssl-key key.key --ssl-cert cert.crt -l 8070 build",
    "build": "npm run cjs && npm run rollup:esm && npm run rollup:babel && npm run min && npm run size",
    "cjs": "ascjs esm cjs",
    "rollup:esm": "rollup --config rollup/esm.config.js",
    "rollup:babel": "rollup --config rollup/babel.config.js && sed -i.bck 's/^var /self./' index.js && rm -rf index.js.bck",
    "min": "terser index.js --comments='/^!/' -c -m -o min.js",
    "coveralls": "c8 report --reporter=text-lcov | coveralls",
    "size": "cat min.js | brotli | wc -c && cat esm/index.js | brotli | wc -c"
  },
  "version": "0.0.18",
  "type": "module",
  "module": "./es.js",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "default": "./cjs/index.js"
    }
  },
  "unpkg": "min.js",
  "name": "rdf-form",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@frogcat/ttl2jsonld": "^0.0.6",
    "@zxcvbn-ts/core": "^1.0.0-beta.0",
    "@zxcvbn-ts/language-common": "^1.0.0-beta.0",
    "@zxcvbn-ts/language-en": "^1.0.0-beta.0",
    "cypress": "^7.0.1",
    "n3": "^1.6.4",
    "quantize": "^1.0.2",
    "tinyduration": "^3.2.0",
    "uhtml": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@ungap/degap": "^0.2.7",
    "ascjs": "^5.0.1",
    "coveralls": "^3.1.1",
    "drop-babel-typeof": "^1.0.3",
    "rollup": "^2.52.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.0",
    "terser": "^5.7.1",
    "ts-node": "*",
    "typescript": "^4.5.5"
  }
}
