{
  "version": "0.1.21",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "scss",
    "demo"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "demo": "cd demo && npm run start",
    "start": "tsdx watch --format esm --noClean",
    "build": "NODE_ENV=production tsdx build --format cjs,esm,umd",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --format cjs,esm,umd",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@frogcat/ttl2jsonld": "^0.0.6",
    "cypress": "^7.0.1",
    "jsonld": "^5.2.0",
    "n3": "^1.6.4",
    "quantize": "^1.0.2",
    "tinyduration": "^3.2.0",
    "uhtml": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rdf-form",
  "author": "Daniel Beeke",
  "module": "dist/rdf-form.esm.js",
  "size-limit": [
    {
      "path": "dist/rdf-form.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rdf-form.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "fast-glob": "^3.2.11",
    "husky": "^7.0.4",
    "rollup-plugin-scss": "^3.0.0",
    "sass": "^1.49.0",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
