@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v:   <http://www.w3.org/2006/vcard/ns#> .
@prefix ui:  <http://www.w3.org/ns/ui#> .

v:nickname rdfs:label "My nickname" .
v:nickname ui:prompt "The nickname you know for this person" .
v:nickname ui:minValue 1 .

v:fn rdfs:label "Full name" .
v:fn ui:prompt "Fully written out name" .
